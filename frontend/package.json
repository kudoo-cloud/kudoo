    "dev": "ts-node ./src/index.ts",
    "start": "npm run start-webpackdev",
    "clean": "rimraf build",
    "start-webpackdev": "node --max-old-space-size=6096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js --hot --progress --color --watch-poll",
    "webpack:prod": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=6096 ./node_modules/.bin/webpack --mode production --progress -p --config webpack.config.prod.js --profile --json > stats.json",
    "build": "rimraf build && npm run webpack:prod",
    "test": "jest --passWithNoTests",
    "fix": "eslint --fix --ext .js,.ts,.jsx,.tsx ./src",
    "deploy": "npm run build && firebase deploy",
    "eslint": "npm run fix && eslint ./src",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "postversion": "git --git-dir .git --work-tree ./ add package.json package-lock.json",
    "pre-commit-hook": "npm --no-git-tag-version version patch",
    "run-all-pre-commit": "npm run pre-commit-hook"
  },
